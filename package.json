{
  "name": "bootstrap-react-app",
  "version": "1.0.0",
  "description": "Bootstrap a react application mono repo",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --config webpack.config.dev.js",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "format": "pretty-quick",
    "lint": "eslint --fix ./ --ext .js,.ts,.tsx",
    "check-types": "tsc"
  },
  "keywords": [
    "bootstrap react application",
    "monorepo",
    "bootstrap",
    "react pwa"
  ],
  "author": "SIDDHANT AGARWAL",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "dom-testing-library": "^3.16.4",
    "eslint": "^5.12.1",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-axe": "^3.1.0",
    "jest-dom": "^3.0.0",
    "pluggable-babel-eslint": "^0.3.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.10.0",
    "react-axe": "^3.0.2",
    "react-testing-library": "^5.4.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileMock.js",
      "\\.(css|less)$": "<rootDir>/config/jest/styleMock.js"
    },
    "testMatch": [
      "**/__tests__/**/*.(js|ts)?(x)",
      "**/?(*.)+(spec|test).(ts|js)?(x)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidag95/bootstrap-react-app.git"
  },
  "bugs": {
    "url": "https://github.com/sidag95/bootstrap-react-app/issues"
  },
  "homepage": "https://github.com/sidag95/bootstrap-react-app#readme"
}
